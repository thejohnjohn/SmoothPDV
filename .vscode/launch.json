{
    "version": "0.2.0",
    "configurations": [ 
                {
            "type": "node",
            "request": "attach",
            "name": "SmoothPDV API Debugger",
            "processId": "${command:PickProcess}",
            // "protocol": "inspector",
            "restart": true,
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
                "**/internal/process/**",
                "**/internal/async_hooks.js",
                "**/internal/process/task_queues.js",
                "**/internal/process/promises.js"
            ]
        }
    ]
}